{
  "name": "zadcalc",
  "version": "2.2.0",
  "description": "Here is a Node package that receives a expression as string and returns the result.",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "type": "commonjs",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "start": "node src/modules/main.js",
    "build": "rm -rf dist && mkdir -p dist && find src -name '*.js' -exec sh -c 'target=\"dist/${1#src/}\"; mkdir -p \"$(dirname \"$target\")\"; terser \"$1\" --compress --mangle -o \"$target\"' _ {} \\; && cp src/main.mjs dist/main.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zadoque/zadcalc.git"
  },
  "keywords": [
    "math",
    "calculator",
    "expression",
    "eval"
  ],
  "author": "Zadoque Carneiro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Zadoque/zadcalc/issues"
  },
  "homepage": "https://github.com/Zadoque/zadcalc#readme",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "eslint": "^9.20.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-snakecasejs": "^2.2.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "terser": "^5.39.0"
  },
  "files": [
    "dist/",
    "README.md"
  ]
}
